{
    "name": "simplify LMS",
    "version": "1.0.8",
    "manifest_version": 3,
    "permissions": [
        "storage"
      ],
    "description": "simplify your LMS(Kanazawa University)",
    "action": {
      "default_title": "simplify LMS",
      "default_icon": {
        "16" : "icons/image(16).png",
        "32" : "icons/image(32).png",
        "48" : "icons/image(48).png",
        "128" : "icons/image(128).png" 
      }
    },
    "icons" : {
      "16" : "icons/image(16).png",
      "32" : "icons/image(32).png",
      "48" : "icons/image(48).png",
      "128" : "icons/image(128).png"
    },
    "content_scripts": [{
      "matches": [
        "https://eduweb.sta.kanazawa-u.ac.jp/portal/StudentApp/Regist/RegistList.aspx",
        "https://eduweb.sta.kanazawa-u.ac.jp/portal/StudentApp/Blank.aspx",
        "https://acanthus.cis.kanazawa-u.ac.jp/base/lms-course/list"],
      "js": [
        "jquery-3.6.0.min.js",
        "main.js"
      ]
    }]
  }



  